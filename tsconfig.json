{
  "compilerOptions": {
    "lib": ["es6", "dom"],
    "types": ["react", "react-native"],
    "module": "commonjs",
    "target": "es5",
    "moduleResolution": "node",
    "declaration": true,
    "rootDir": "src",
    "strict": true,
    "noImplicitReturns": true,
    "removeComments": false,
    "jsx": "react",
    "sourceMap": true,
    "skipLibCheck": true,
    "esModuleInterop": true
  }
}
